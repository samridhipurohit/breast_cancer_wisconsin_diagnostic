```{r, here-i-am, include=FALSE}
#! TO DO:
#!   add call to here::i_am
library(here)
here::i_am("report.Rmd")
```

```{r, load-random-numbers, include = FALSE}
#! TO DO: 
#!   read random_numbers1 from output1 directory
random_numbers1 <- readRDS(here::here("output1", "random_numbers1.rds"))

#! TO DO: 
#!   read random_numbers2 from output2 directory
random_numbers2 <- readRDS(here::here("output2", "random_numbers2.rds"))
 
#! TO DO: 
#!   read random_numbers3 from output3 directory
random_numbers3 <- readRDS(here::here("output3", "random_numbers3.rds"))
```

# Random numbers 1

```{r, random-numbers1-histogram, echo=FALSE}
#| fig.cap = "First set of random numbers"
hist(random_numbers1, main = "Histogram of Random Numbers 1", col="lightblue")
```

# Random numbers 2

```{r, random-numbers2-histogram, echo=FALSE}
#| fig.cap = "Second set of random numbers"
hist(random_numbers2, main = "Histogram of Random Numbers 2", col="lightpink")
```

# Random numbers 3

```{r, random-numbers3-histogram, echo=FALSE}
#| fig.cap = "Third set of random numbers"
hist(random_numbers3, main = "Histogram of Random Numbers 3", col="lightyellow")
```
